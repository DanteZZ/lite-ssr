    "build": "pnpm build:core && pnpm build:vue && pnpm build:lssr && pnpm build:proxy && pnpm build:vue-unhead && pnpm build:cached-data",
    "build:core": "pnpm --filter core build",
    "build:lssr": "pnpm --filter lite-ssr build",
    "build:vue": "pnpm --filter vue build",
    "build:proxy": "pnpm --filter proxy build",
    "build:vue-unhead": "pnpm --filter vue-unhead build",
