{
  "name": "lite-ssr",
  "version": "0.2.5",
  "description": "Simple vite ssr server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "lssr": "./dist/bin.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start:example": "tsc && node ./dist/bin.js",
    "build:example": "tsc && node ./dist/bin.js --build",
    "serve:example": "tsc && node ./dist/bin.js --serve ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "tsc",
    "release": "standard-version",
    "release:pre": "npm run release -- --prerelease",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:push": "git push --follow-tags origin main"
  },
  "author": "Zebrains Dev",
  "license": "MIT",
  "files": [
    "README.md",
    "src",
    "dist",
    "index.html"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.zebrains.team/frontend/modules/lite-ssr.git"
  },
  "bugs": {
    "url": "https://gitlab.zebrains.team/frontend/modules/lite-ssr/-/issues"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "fix-esm-import-path": "^1.10.1",
    "rollup": "^4.11.0",
    "standard-version": "^9.5.0",
    "ts-loader": "^9.5.1"
  },
  "peerDependencies": {
    "typescript": "^5.3.3",
    "vite": "^5.4.1"
  },
  "dependencies": {
    "@lite-ssr/core": "workspace:*",
    "@types/compression": "^1.7.5",
    "chalk": "^5.3.0",
    "compression": "^1.7.5",
    "express": "^4.18.2",
    "sirv": "^3.0.0"
  }
}
